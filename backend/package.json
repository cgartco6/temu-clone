{
  "name": "temu-clone-backend",
  "version": "1.0.0",
  "description": "Complete E-commerce Platform Backend",
  "main": "src/app.js",
  "scripts": {
    "start": "node src/app.js",
    "dev": "nodemon src/app.js",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:e2e": "jest --config jest.e2e.config.js",
    "lint": "eslint src/**/*.js",
    "lint:fix": "eslint src/**/*.js --fix",
    "format": "prettier --write src/**/*.js",
    "migrate": "node scripts/migrate.js",
    "migrate:up": "node scripts/migrate.js up",
    "migrate:down": "node scripts/migrate.js down",
    "seed": "node scripts/seed.js",
    "seed:prod": "NODE_ENV=production node scripts/seed.js",
    "clean": "rm -rf coverage dist",
    "build": "npm run clean && npm run lint && npm test",
    "docker:build": "docker build -t temu-backend .",
    "docker:run": "docker run -p 5000:5000 temu-backend"
  },
  "keywords": [
    "ecommerce",
    "express",
    "mongodb",
    "nodejs",
    "temu"
  ],
  "author": "Your Name <your.email@example.com>",
  "license": "MIT",
  "dependencies": {
