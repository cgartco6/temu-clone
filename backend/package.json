    "helmet": "^7.0.0",
    "compression": "^1.7.4",
    "express-rate-limit": "^6.10.0",
    "express-validator": "^7.0.1",
    "express-mongo-sanitize": "^2.2.0",
    "xss-clean": "^0.1.4",
    "multer": "^1.4.5-lts.1",
    "sharp": "^0.32.5",
    "aws-sdk": "^2.1433.0",
    "stripe": "^13.5.0",
    "paypal-rest-sdk": "^1.8.1",
    "twilio": "^4.13.0",
    "node-cron": "^3.0.3",
    "socket.io": "^4.7.0",
    "winston": "^3.10.0",
    "winston-daily-rotate-file": "^4.7.1",
    "axios": "^1.5.0",
    "dotenv": "^16.3.1",
    "moment": "^2.29.4",
